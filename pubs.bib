@article{minollariRedfigureVasesDurres2018,
	title = {Red-figure vases from {Durres} – {A} reflection of a local culture?},
	volume = {21},
	issn = {2352-409X},
	url = {https://www.dropbox.com/s/k40nfs2u7sjs138/Minollari%20-%20Red-figure%20vases%20from%20Durres%E2%80%93Areflection%20of%20a%20local%20culture.pdf?dl=0},
	doi = {10.1016/j.jasrep.2016.12.007},
	abstract = {Epidamnos/Dyrrhachion was one of the most important Greek colonies in the territory of modern Albania and in its necropolis a lot of red-figure ceramics was found. Many of these objects were very probably produced locally. The methods and style of the decoration of these ceramics was inspired by the South Italian production, however they have some specific characteristics: small size, limited iconography, and often low quality of execution. Many of them are decorated with the images of women, some as part of Dionysiac scenes, and some in everyday situations. I conduct a visual analysis of these images and vessels, asking the question whether or not they reflect the local culture and possible Illyrian influences. I especially focus on the question why these vases were interesting for the local consumer. The result of my inquiry is that pottery from Epidamnos does not reveal images of local costumes, nor new iconography. On the other hand, the frequent appearance of female figures, or scenes of nude athletes with women, can be interpreted as reflecting local ideology. The repetitiveness of the images speaks about an increased demand on the side of the consumers, which points to the significance of Illyrian population in the economic and social life of the coastal city. I suggest that use of red-figure pottery in funeral rituals was considered an expression of identity and status of middle class citizens of Epidamnos.},
	language = {en},
	urldate = {2020-03-18},
	journal = {Journal of Archaeological Science: Reports},
	author = {Minollari, Katarzyna},
	month = oct,
	year = {2018},
	pages = {1025--1034}
}

